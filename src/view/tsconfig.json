{
    "compilerOptions": {
        "module": "esnext",
        "moduleResolution": "node",
        "target": "ES2020",
        "outDir": "configViewer",
        "lib": [
            "ES2020",
            "dom"
        ],  
        "jsx": "react",
        "sourceMap": true,
        "rootDir": "..",
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "experimentalDecorators": true
    },
    "exclude": [
        "node_modules"
    ]
}