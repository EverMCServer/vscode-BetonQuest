{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "Node",
        "target": "ES5",
        "outDir": "conversationEditor",
        "lib": [
            "ES5",
            "DOM"
        ],
        "jsx": "react",
        "sourceMap": true,
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "experimentalDecorators": true
    },
    "exclude": [
        "node_modules"
    ]
}